{% extends 'base.html.twig' %} {% block title %}Pokaż wszystkich użytkowników{% endblock %} {% block body %}
<style>
    .example-wrapper {
        margin: 1em auto;
        max-width: 800px;
        width: 95%;
        font: 18px/1.5 sans-serif;
    }

    .example-wrapper code {
        background: #F5F5F5;
        padding: 2px 6px;
    }
</style>

<div class="example-wrapper">
    Ilosc użytkowników: {{users | length}}<br>
   <ul>
        {% for user in users %}
            <li>{{ user.getLogin }}</li>
       {% else %}
            Brak użytkowników.
        {% endfor %}
    </ul>
</div>
{% endblock %}